
@{
    ViewBag.Title = "Top Websites";
}

@section styles {
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/bs-3.3.7/dt-1.10.15/b-1.3.1/b-html5-1.3.1/b-print-1.3.1/datatables.min.css" />
    @Styles.Render("~/Content/search-listtopwebsites")
}

@section scripts {
    <script type="text/javascript" src="https://cdn.datatables.net/v/bs-3.3.7/dt-1.10.15/b-1.3.1/b-html5-1.3.1/b-print-1.3.1/datatables.min.js"></script>
    @Scripts.Render("~/bundles/search-listtopwebsites")
}

<h2>Top Websites</h2>

<table id="top-results-grid" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Date</th>
            <th>Website</th>
            <th>Total Visits</th>
        </tr>
    </thead>
</table>