
@{
    ViewBag.Title = "Top Five Websites";
}

@section styles {
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/bs/dt-1.10.15/datatables.css" />
}

@section scripts {
    <script type="text/javascript" src="https://cdn.datatables.net/v/bs/dt-1.10.15/datatables.js"></script>
    @Scripts.Render("~/bundles/search.listtopwebsites")
}

<h2>Top Five Websites</h2>

<table id="top-results-grid" class="display" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>Date</th>
            <th>Website</th>
            <th>Total Visits</th>
        </tr>
    </thead>
</table>